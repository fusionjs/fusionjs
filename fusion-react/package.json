{
  "name": "fusion-react",
  "description": "FusionJS entry point for React universal rendering",
  "version": "0.0.0-monorepo",
  "browser": {
    "./dist-node-cjs/index.js": "./dist-browser-cjs/index.js",
    "./dist-node-esm/index.js": "./dist-browser-esm/index.js"
  },
  "dependencies": {
    "@derekju/react-ssr-prepass": "^1.5.0",
    "prop-types": "^15.7.2",
    "react-is": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-flow-strip-types": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "create-universal-package": "^4.3.0",
    "flow-bin": "0.131.0",
    "fusion-core": "workspace:*",
    "fusion-test-utils": "workspace:*",
    "fusion-tokens": "workspace:*",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "just-compose": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "files": [
    "dist-*/",
    "src/",
    "index.d.ts"
  ],
  "homepage": "https://fusionjs.com/api/fusion-react",
  "license": "MIT",
  "main": "./dist-node-cjs/index.js",
  "types": "./index.d.ts",
  "module": "./dist-node-esm/index.js",
  "peerDependencies": {
    "fusion-core": "workspace:*",
    "fusion-tokens": "workspace:*",
    "react": ">= 18.0.0",
    "react-dom": ">= 18.0.0"
  },
  "repository": {
    "directory": "fusion-react",
    "type": "git",
    "url": "https://github.com/fusionjs/fusionjs"
  },
  "scripts": {
    "clean": "cup-clean",
    "flow": "flow",
    "lint": "yarn g:lint",
    "prepack": "cup-build --force-flow",
    "test": "jest",
    "view-cover": "npm run cover && open coverage/index.html"
  },
  "sideEffects": false
}
