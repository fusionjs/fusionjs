{
  "name": "fusion-plugin-introspect",
  "description": "Visualize the tree of plugins in your app",
  "version": "0.0.0-monorepo",
  "bin": {
    "fusion-run-introspect": "src/cli/index.js"
  },
  "browser": {
    "./dist-node-cjs/index.js": "./dist-browser-cjs/index.js",
    "./dist-node-esm/index.js": "./dist-browser-esm/index.js"
  },
  "dependencies": {
    "koa-bodyparser": "4.2.1",
    "sade": "^1.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-flow-strip-types": "^7.14.5",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "28.1.3",
    "create-universal-package": "^4.3.0",
    "flow-bin": "0.131.0",
    "fusion-cli": "workspace:*",
    "fusion-core": "workspace:*",
    "fusion-test-utils": "workspace:*",
    "get-port": "^5.1.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "puppeteer": "^2.1.1"
  },
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "files": [
    "dist-*/",
    "src/",
    "index.d.ts"
  ],
  "homepage": "https://fusionjs.com/api/fusion-plugin-introspect",
  "license": "MIT",
  "main": "./dist-node-cjs/index.js",
  "types": "./index.d.ts",
  "module": "./dist-node-esm/index.js",
  "peerDependencies": {
    "fusion-core": "workspace:*"
  },
  "repository": {
    "directory": "fusion-plugin-introspect",
    "type": "git",
    "url": "https://github.com/fusionjs/fusionjs"
  },
  "scripts": {
    "clean": "cup-clean",
    "flow": "flow",
    "lint": "yarn g:lint",
    "prepack": "cup-build --force-flow",
    "test": "jest",
    "view-cover": "npm run cover && open coverage/index.html"
  },
  "sideEffects": false
}
